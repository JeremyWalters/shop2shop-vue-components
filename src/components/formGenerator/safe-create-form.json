{
	"name": "safe-create",
	"applyToAllFields": {
		"htmlClass": "xs12 sm6"
	},
	"layout": {
		"properties": {
			"d-block": true
		}
	},
	"fields": [
		{
			"label": "Active",
			"name": "active",
			"component": "v-checkbox"
		},
		{
			"label": "Description",
			"name": "description",
			"component": "v-text-field",
			"required": true
		},
		{
			"label": "Select Organisation",
			"name": "organisation",
			"component": "v-autocomplete",
			"required": true,
			"items": "organisations"
		},
		{
			"label": "Safe Serial",
			"name": "safeSerial",
			"component": "v-text-field",
			"required": true
		},
		{
			"label": "Safe Type",
			"name": "safeType",
			"component": "v-select",
			"required": true,
			"items": "safeTypes"
		},
		{
			"label": "Vendor",
			"name": "vendor",
			"component": "v-select",
			"items": "vendors",
			"required": true
		}
	]
}
