{
	"name": "shop2shop-vue-components",
	"version": "0.2.3",
	"main": "./dist/shop2shop.common.js",
	"scripts": {
		"serve": "vue-cli-service serve",
		"gen-schema": "typescript-json-schema src/components/formGenerator/formGeneratedTypes.ts --required --out ./src/components/formGenerator/formGeneratorSchema.json Form",
		"build": "vue-cli-service build",
		"build-bundle": "vue-cli-service build --target lib --name shop2shop ./src/components/index.ts",
		"lint": "vue-cli-service lint"
	},
	"dependencies": {
		"ajv": "^6.7.0",
		"vue": "^2.5.22",
